{
  "name": "yazo-knowledge-api",
  "version": "1.0.0",
  "description": "Serverless yazo-knowledge-api base",
  "keywords": [
    "typescript",
    "base",
    "serverless",
    "lambda"
  ],
  "author": {
    "name": "Gabriel Maia",
    "url": "https://github.com/gabrielmaialva33"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielmaialva33/yazo-knowledge-api.git"
  },
  "private": true,
  "license": "MIT",
  "main": "serverless.ts",
  "scripts": {
    "dev": "sls offline",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "@typescript-eslint/naming-convention": "off"
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "dependencies": {
    "@middy/core": "^3.1.0",
    "@middy/http-json-body-parser": "^3.1.0",
    "luxon": "^2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.19.0",
    "@types/aws-lambda": "^8.10.101",
    "@types/luxon": "^2.3.2",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "esbuild": "^0.14.47",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "json-schema-to-ts": "^2.5.3",
    "prettier": "^2.7.1",
    "serverless": "^3.19.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.30.0",
    "serverless-offline": "^8.8.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "yarn-upgrade-all": "^0.7.1"
  }
}
